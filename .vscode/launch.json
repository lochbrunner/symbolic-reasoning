    "version": "0.2.0",
    "configurations": [
        {
            "name": "core test",
            "type": "lldb",
            "request": "launch",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--lib=core",
                    "--package=core",
                ],
                "filter": {
                    "name": "core",
                    "kind": "lib"
                }
            },
            "args": [
                "--test",
                "function_with_multiple_args",
                "--nocapture"
            ],
            "sourceLanguages": [
                "rust"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "pycore",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/pycore/test/run_tests.py",
            "args": [
                "-v"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "justMyCode": false
        },
        {
